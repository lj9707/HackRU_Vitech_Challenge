<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>HackRU</title>

    <!-- Bootstrap core CSS -->
    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <style>
      body {
        padding-top: 54px;
      }
      @media (min-width: 992px) {
        body {
          padding-top: 56px;
        }
      }
    </style>

    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.7.1/css/bootstrap-datepicker.css"></script>

  </head>

  <body>

    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
      <div class="container">
        <a class="navbar-brand" href="#">HackRU</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="/index">Home
              </a>
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="predict">Machine Learning
                <span class="sr-only">(current)</span>
              </a>
            </li>
          </ul>
        </div>
      </div>
    </nav>

    <!-- Page Content -->
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center mt-5">
          <h1 class="mt-5">HackRU Project</h1>
          <p class="lead"><%=status%></p>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12">
          <form class="form-horizontal" action="/predict" method="post">
            <fieldset>

            <!-- Form Name -->
            <legend>Form Name</legend>

            <!-- Text input-->
            <div class="form-group">
              <label for="marital_status" class="col-md-4 control-label">Marital Status
                <select class="form-control input-md" id="marital_status" name="marital_status">
                  <option value="0">Single</option>
                  <option value="1">Married</option>
                </select>
              </label>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label for="sex" class="col-md-4 control-label">Gender
                <select class="form-control input-md" id="sex" name="sex">
                  <option value="0">Male</option>
                  <option value="1">Female</option>
                </select>
              </label>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="date_added">Date Added</label>
              <div class="col-md-4">
              <input id="date_added" name="date_added" type="datetime-local" placeholder="2015-01-07" class="form-control input-md" required="">

              </div>
            </div>

            <div class="form-group">
              <label class="col-md-4 control-label" for="birth_date">Birth Date</label>
              <div class="col-md-4">
                <input id="birth_date" required="" type="datetime-local" name="birth_date" class="form-control input-md">
              </div>
            </div>

              <div class="form-group">
                <label class="col-md-4 control-label" for="policy_start_date">Policy Start Date</label>
                <div class="col-md-4">
                  <input id="policy_start_date" required="" type="datetime-local" name="policy_start_date" class="form-control input-md">
                </div>
              </div>
            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="longitude">Longitude</label>
              <div class="col-md-4">
              <input id="longitude" name="longitude" type="text" placeholder="-63.6219" class="form-control input-md" required="">

              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="latitude">Latitude</label>
              <div class="col-md-4">
              <input id="latitude" name="latitude" type="text" placeholder="44.6327" class="form-control input-md" required="">

              </div>
            </div>

            <!-- Search input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="state">State</label>
              <div class="col-md-4">
                <select class="form-control input-md" id="state" name="state">
                  <option value="0">Nova Scotia</option>
                  <option value="0.25">Northwest Territory</option>
                  <option value="0.5">Nunavut Territory</option>
                  <option value="0.75">Alberta</option>
                  <option value="1">British Columbia</option>
                  <option value="1.25">Ontario</option>
                  <option value="1.5">Quebec</option>
                  <option value="1.75">Saskatchen</option>
                  <option value="2">Yukon</option>
                  <option value="2.25">Prince Edward Island</option>
                  <option value="2.5">Manitoba</option>
                  <option value="2.75">New Brunswick</option>
                  <option value="3">Newfoundland</option>
                </select>
              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="coverage">Insurance Coverage</label>
              <div class="col-md-4">
                <select class="form-control input-md" id="coverage" name="coverage">
                  <option value="0">Single</option>
                  <option value="1">Family</option>
                </select>
              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="premium">Insurance Premium</label>
              <div class="col-md-4">
              <input id="premium" name="premium" type="text" placeholder="18" class="form-control input-md" required="">

              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="plan">Insurance Plan</label>
              <div class="col-md-4">
                <select class="form-control input-md" id="plan" name="plan">
                  <option value="0">Gold</option>
                  <option value="1">Silver</option>
                  <option value="2">Regular</option>
                </select>
              </div>
            </div>
              

            <!-- Button -->
            <div class="form-group">
              <label class="col-md-4 control-label" for="submit">Predict</label>
              <div class="col-md-4">
                <button id="submit" name="submit" class="btn btn-primary">Submit</button>
              </div>
            </div>

            </fieldset>
          </form>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12 text-center" style="margin-top:30px">
          <ul class="list-unstyled">
            <li></li>
            <li></li>
          </ul>
        </div>
      </div>
    </div>

    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/popper/popper.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>

  </body>

</html>
